<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.txystem.checkin" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CheckIN</name>
    <description>
        Um aplicativo de Produtivade Financeira
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        TXystem Enterprise
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/checkin-icon.png" />
</widget>
